}

@inproceedings{duan_full_2024,
	address = {New York, NY, USA},
	series = {{ISAIMS} '23},
	title = {The {Full} {Intelligent} {Stakeholder} {Value} {Network} {Model} and {Its} {Empirical} {Results} {Based} on {Value} {Stream} in {Hospital}},
	isbn = {979-8-4007-0813-8},
	url = {https://doi.org/10.1145/3644116.3644301},
	doi = {10.1145/3644116.3644301},
	abstract = {Medical examinations are an important part of a patient's hospital visit process. The long waiting time for examination and the uncertainty of examination time will cause patients to question the service and management level of the hospital, which will lead to a decline in the overall satisfaction of patients with medical treatment. By discussing the patient- centered examination appointment process, We established a fully intelligent stakeholder value network based on the examination appointment management value stream with clinical, medical technology, information, and patients as the relevant stakeholders, and optimized and reorganized the original examination and consultation process by incorporating smart appointments into business process management. Combined with “value recognition and benefit sharing”, the practical exploration of the “value stream” management model of intelligent inspection appointments in hospitals was realized. Through the comparison of the process before and after the stakeholder value network platform was launched and the analysis of the effectiveness of inspection reservations, it was concluded that the intelligent reservation platform of the whole hospital could reasonably disperse the inspection time and effectively shorten the time- consuming of patients queuing. The hospital had effectively added value in terms of patient-centered “knowledge service” and “product service”. The intuitive performance of users’ perception of effective value was an improvement in overall satisfaction, and it also had a positive and obvious feedback effect on hospital operation efficiency.},
	booktitle = {Proceedings of the 2023 4th {International} {Symposium} on {Artificial} {Intelligence} for {Medicine} {Science}},
	publisher = {Association for Computing Machinery},
	author = {Duan, Qinghong and Chai, Huan and Wu, Tao and Ouyang, Weiwei and Lei, Ying and Zhao, Xu and Zhao, Zexin and Jin, Yan and Zhou, Zhongfei and Chen, Jianping},
	year = {2024},
	note = {event-place: Chengdu, China},
